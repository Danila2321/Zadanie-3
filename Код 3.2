n = int(input())

arr = list(map(int, input().split()))

result = [-1] * n

for i in range(n):
    found = False
    for j in range(i + 1, n):
        if arr[j] < arr[i]:
            result[i] = j
            found = True
            break

print(f"Кол-во элементов в массиве: '{n}'")
print(f"Массив: '{arr}'")
print(' '.join(map(str, result)))
